<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <padding android:bottom="10dp" android:left="10dp" android:top="10dp" />
            <corners android:radius="5dp" />
            <solid android:color="@color/white" />
            <stroke android:width="1dp" android:color="#da4201" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <padding android:bottom="10dp" android:left="10dp" android:top="10dp" />
            <corners android:radius="5dp" />
            <solid android:color="@color/white" />
            <stroke android:width="1dp" android:color="#ff7a15" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <padding android:bottom="10dp" android:left="10dp" android:top="10dp" />
            <corners android:radius="5dp" />
            <solid android:color="#ffefd5" />
            <stroke android:width="1dp" android:color="#fcbf5c" />
        </shape>
    </item>
</selector>